{
  "name": "graphql-auth",
  "license": "MIT",
  "scripts": {
    "cnt": "npx cnt -mq -f -o",
    "postinstall": "typesync",
    "predev": "npm run generate:prisma",
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn --transpileOnly ./src",
    "prebuild": "cross-env NODE_ENV=production npm run -s clean && npm run -s generate:prisma && npm run -s generate:nexus",
    "build": "cross-env NODE_ENV=production tsc",
    "seed": "ts-node prisma/seed.ts",
    "clean": "rimraf dist",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only ./src/schema",
    "db:save": "prisma2 lift save --name",
    "db:migrate": "prisma2 lift up",
    "start": "cross-env NODE_ENV=production node dist/src/index.js"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview021",
    "apollo-server": "^2.14.2",
    "bcrypt": "3.0.8",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "graphql": "^14.5.8",
    "graphql-shield": "7.0.11",
    "graphql-yoga": "1.18.3",
    "helmet": "^3.21.1",
    "jsonwebtoken": "8.5.1",
    "nexus": "0.12.0-rc.9",
    "nexus-prisma": "^0.9.1",
    "nodemailer": "^6.4.2"
  },
  "devDependencies": {
    "@types/nodemailer-direct-transport": "^1.0.31",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/bcrypt": "3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.1.3",
    "@types/helmet": "^0.0.45",
    "@types/jsonwebtoken": "8.3.7",
    "@types/node": "^13.1.6",
    "@types/nodemailer": "^6.4.0",
    "@types/rimraf": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.3.1",
    "create-nexus-type": "^1.1.2",
    "cross-env": "^7.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.8.0",
    "prisma2": "2.0.0-preview021",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.0",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.5",
    "typesync": "^0.6.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
